{
  "name": "esri-global-nav",
  "version": "1.0.0",
  "scripts": {
    "clean": "git clean -X -d -f",
    "start": "gulp --gulpfile node_modules/gulp-config-dev --cwd",
    "test": "echo 'Running tests...'; npm run test:css && npm run test:js",
    "test:css": "stylelint **/*.css **/dependent-css/*.css **/dependent-css/**/*.css --syntax scss",
    "test:js": "eslint **/*.js **/dependent-js/*.js **/dependent-js/**/*.js --cache --ignore-pattern .gitignore"
  },
  "devDependencies": {
    "eslint": "^4.2.0",
    "eslint-config-dev": "^2.0.0",
    "gulp-config-dev": "^3.9.1",
    "pre-commit": "^1.2.2",
    "stylelint": "^8.0.0",
    "stylelint-config-dev": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "dev"
  },
  "stylelint": {
    "extends": "stylelint-config-dev"
  },
  "private": true
}
