{
  "name": "esri-global-nav",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "context": "index.json",
  "style": "index.css",
  "template": "index.html",
  "scripts": {
    "lint": "npm run lint:ec && npm run lint:css && npm run lint:js",
    "lint:ec": "echint",
    "lint:css": "stylelint *.css css/*.css",
    "lint:js": "eslint *.js --cache --ignore-pattern .gitignore",
    "start": "rapid",
    "test": "npm run lint"
  },
  "devDependencies": {
    "echint": "^2.1.0",
    "echint-config-dev": "1.0.0",
    "eslint": "^3.8.1",
    "eslint-config-dev": "^1.0.0",
    "pre-commit": "^1.2.2",
    "rapid": "github:jonathantneal/rapid",
    "rapid-config-dev": "github:jonathantneal/rapid-config-dev",
    "stylelint": "^7.7.1",
    "stylelint-config-dev": "^1.0.0"
  },
  "eslintConfig": {
    "extends": "dev"
  },
  "pre-commit": [
    "lint"
  ],
  "rapid": {
    "extends": "dev"
  },
  "stylelint": {
    "extends": "stylelint-config-dev"
  }
}
