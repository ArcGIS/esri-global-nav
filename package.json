{
  "name": "esri-global-nav",
  "version": "1.0.0",
  "main": "gnav.js",
  "style": "gnav.css",
  "scripts": {
    "clean": "git clean -X -d -f",
    "rapid": "rapid",
    "start": "gulp",
    "test": "npm run test:css && npm run test:js",
    "test:css": "stylelint *.css css/*.css",
    "test:js": "eslint *.js --cache --ignore-pattern .gitignore"
  },
  "dependencies": {
    "element-closest": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-dev": "^2.0.0",
    "eslit": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "gulp-postcss": "^6.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.0",
    "pre-commit": "^1.2.2",
    "rapid": "^3.1.0",
    "rapid-config-dev": "^1.2.0",
    "rollup-stream": "^1.19.0",
    "stylelint": "^7.10.1",
    "stylelint-config-dev": "^1.2.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "eslintConfig": {
    "extends": "dev"
  },
  "pre-commit": [
    "test"
  ],
  "rapid": {
    "extends": "dev",
    "html": {
      "from": "placeholders/demo.html",
      "before": ""
    },
    "css": {
      "from": "placeholders/demo.css"
    },
    "js": {
      "from": "placeholders/demo.js"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-dev"
  },
  "private": true
}
