{
  "name": "@esri/global-nav",
  "version": "2.0.0",
  "description": "Navigation components for Esri",
  "author": "Esri Web Applications <webapps@esri.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "jsnext:main": "dist/esm/index.mjs",
  "unpkg": "dist/calcite.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs-readme",
    "start": "stencil build --dev --watch --serve --docs-readme",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "prerelease:prepare": "npm run build",
    "prettier": "npm-run-all --parallel prettier:**",
    "prettier:ts": "prettier --write \"src/**/*.ts?(x)\"",
    "prettier:styles": "prettier --write \"src/**/*.scss\"",
    "release:prepare": "npm version prerelease --preid=alpha --no-git-tag-version",
    "release:publish": "./release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/global-nav.git"
  },
  "dependencies": {
    "@esri/calcite-base": "^1.1.0",
    "@esri/calcite-colors": "^1.4.1",
    "@esri/calcite-ui-icons": "^2.5.0",
    "npm-run-all": "4.1.5"
  },
  "devDependencies": {
    "@stencil/core": "one",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^0.2.3",
    "@stencil/state-tunnel": "one",
    "@types/jest": "^23.3.14",
    "@types/puppeteer": "1.12.4",
    "gh-release": "^3.5.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "prettier": "1.18.2",
    "puppeteer": "1.14.0"
  },
  "license": "Apache-2.0"
}
